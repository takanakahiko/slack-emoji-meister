{
  "name": "slack-emoji-meister",
  "private": true,
  "version": "0.0.2",
  "description": "Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.",
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp pack --production --vendor=chrome",
    "build:firefox": "gulp pack --production --vendor=firefox",
    "build:opera": "gulp pack --production --vendor=opera",
    "build:edge": "gulp pack --production --vendor=edge",
    "dev": "npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera",
    "dev:edge": "gulp --watch --vendor=edge",
    "lint": "standard tasks/**/*.js && tsc --noEmit && tslint ./app/scripts/**/*.ts --fix",
    "deploy": "npm run upload-webstore && npm run publish-webstore",
    "upload-webstore": "webstore upload --source packages/new.zip --extension-id $EXTENSION_ID --client-id $CLIENT_ID --client-secret $CLIENT_SECRET --refresh-token $REFRESH_TOKEN",
    "publish-webstore": "webstore publish --extension-id elomekmlfonmdhmpmdfldcjgdoacjcba"
  },
  "standard": {
    "globals": [
      "chrome"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/register": "7.0.0",
    "@types/chrome": "^0.x.x",
    "@types/node": "10.12.18",
    "ansi-colors": "3.2.3",
    "chai": "4.x.x",
    "chrome-webstore-upload-cli": "1.2.0",
    "chromereload": "0.x.x",
    "debounce": "1.x.x",
    "del": "3.x.x",
    "fancy-log": "1.3.3",
    "gulp": "4.0.0",
    "gulp-bump": "3.x.x",
    "gulp-cache": "1.x.x",
    "gulp-clean-css": "^4.x.x",
    "gulp-filter": "^5.x.x",
    "gulp-git": "^2.x.x",
    "gulp-if": "2.x.x",
    "gulp-imagemin": "5.0.3",
    "gulp-json-transform": "0.x.x",
    "gulp-less": "4.0.1",
    "gulp-livereload": "4.0.1",
    "gulp-plumber": "1.x.x",
    "gulp-rename": "1.4.0",
    "gulp-sass": "^4.x.x",
    "gulp-sourcemaps": "^2.x.x",
    "gulp-tag-version": "1.x.x",
    "gulp-zip": "^4.x.x",
    "require-dir": "1.x.x",
    "standard": "^12.0.1",
    "ts-loader": "^5.x.x",
    "tslint": "^5.x.x",
    "typescript": "3.2.2",
    "undertaker-forward-reference": "1.0.2",
    "vinyl-named": "1.x.x",
    "webpack": "4.x.x",
    "webpack-stream": "5.x.x",
    "yargs": "^12.x.x"
  }
}
