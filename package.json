{
  "name": "slack-emoji-meister",
  "private": true,
  "version": "0.2.0",
  "description": "Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.",
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp pack --production --vendor=chrome",
    "build:firefox": "gulp pack --production --vendor=firefox",
    "build:opera": "gulp pack --production --vendor=opera",
    "build:edge": "gulp pack --production --vendor=edge",
    "dev": "npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera",
    "dev:edge": "gulp --watch --vendor=edge",
    "lint": "standard tasks/**/*.js && tsc --noEmit && tslint ./app/scripts/**/*.ts --fix",
    "deploy": "npm run upload-webstore && npm run publish-webstore",
    "upload-webstore": "webstore upload --source packages/new.zip  --extension-id $EXTENSION_ID",
    "publish-webstore": "webstore publish --extension-id $EXTENSION_ID"
  },
  "standard": {
    "globals": [
      "chrome"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/register": "7.4.4",
    "@types/chrome": "0.0.86",
    "@types/node": "10.14.7",
    "ansi-colors": "3.2.4",
    "chai": "4.2.0",
    "chrome-webstore-upload-cli": "1.2.0",
    "chromereload": "0.0.2",
    "debounce": "1.2.0",
    "del": "3.0.0",
    "fancy-log": "1.3.3",
    "gulp": "4.0.0",
    "gulp-bump": "3.1.1",
    "gulp-cache": "1.1.1",
    "gulp-clean-css": "4.0.0",
    "gulp-filter": "5.1.0",
    "gulp-git": "2.8.0",
    "gulp-if": "2.0.2",
    "gulp-imagemin": "5.0.3",
    "gulp-json-transform": "0.4.6",
    "gulp-less": "4.0.1",
    "gulp-livereload": "4.0.1",
    "gulp-plumber": "1.2.1",
    "gulp-rename": "1.4.0",
    "gulp-sass": "4.0.2",
    "gulp-sourcemaps": "2.6.4",
    "gulp-tag-version": "1.3.1",
    "gulp-zip": "4.2.0",
    "require-dir": "1.2.0",
    "standard": "12.0.1",
    "ts-loader": "5.3.2",
    "tslint": "5.12.0",
    "typescript": "3.2.2",
    "undertaker-forward-reference": "1.0.2",
    "vinyl-named": "1.1.0",
    "webpack": "4.28.3",
    "webpack-stream": "5.2.1",
    "yargs": "12.0.5"
  }
}
